
struct Point {
	x double;
	y double;
	z double;
}

struct Line {
	start Point;
	end Point;
}

struct Polygon {
	edges Line[];
}

func load() {
	let point Point = Point(1.0, 1.0, 1.0);

	let line Line = Line(
		Point(0.0, 0.0, 0.0),
		point
	);

	let polygon Polygon = Polygon([
		line,
		Line(
			Point(1.0, 0.0, 0.0),
			Point(1.0, 1.0, 0.0)
		),
	]);

	mcb:print(polygon.edges[1].start.x);
}